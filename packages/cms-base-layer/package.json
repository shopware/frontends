    "@vuelidate/core": "2.0.3",
    "@vuelidate/validators": "2.0.4",
    "@vueuse/core": "13.5.0",
    "entities": "6.0.0",
    "html-to-ast": "0.0.6",
    "three": "0.173.0",
    "vue": "3.5.17",
    "xss": "1.0.15"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@nuxt/schema": "4.0.0",
    "@types/three": "0.173.0",
