    "nuxt": "4.0.0",
    "tsconfig": "workspace:*",
    "typescript": "5.8.3",
    "unbuild": "2.0.0",
    "vitest": "3.2.3",
    "vue-router": "4.5.1",
    "vue-tsc": "2.2.10"
  }
