{
  "name": "@shopware-pwa/vue3-plugin",
  "version": "0.1.33",
  "description": "Shopware Fromends Vue3 plugin",
  "author": "Shopware",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shopware/frontends.git"
  },
  "homepage": "https://frontends.shopware.com",
  "keywords": [
    "shopware"
  ],
  "bugs": {
    "url": "https://github.com/shopware/frontends/issues"
  },
  "license": "MIT",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    "import": "./dist/index.mjs"
  },
  "scripts": {
    "build": "unbuild && pnpm build:types",
    "build:types": "tsc ./src/*.ts --declaration --allowJs --emitDeclarationOnly --esModuleInterop --outDir ./temp --skipLibCheck",
    "lint": "eslint src/**/*.ts* --fix --max-warnings=0 && pnpm run typecheck",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "eslint-config-shopware": "workspace:*",
    "tsconfig": "workspace:*",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@shopware-pwa/api-client": "workspace:*",
    "@shopware-pwa/composables-next": "workspace:*",
    "js-cookie": "^3.0.5"
  },
  "peerDependencies": {
    "vue": "^3.3.4"
  }
}
