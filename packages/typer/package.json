{
  "name": "@shopware-pwa/typer",
  "private": true,
  "version": "0.1.11",
  "description": "Generate documentation from types",
  "author": "Shopware",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shopware/frontends.git"
  },
  "homepage": "https://frontends.shopware.com/packages/typer.html",
  "keywords": [
    "shopware"
  ],
  "bugs": {
    "url": "https://github.com/shopware/frontends/issues"
  },
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint src/**/*.ts* --fix --max-warnings=0 && pnpm run typecheck",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "find-in-files": "0.5.0",
    "fs-extra": "11.2.0",
    "ts-dox": "0.1.0",
    "typedoc": "0.25.12",
    "vinyl": "3.0.0",
    "vite": "5.2.6"
  },
  "devDependencies": {
    "@shopware-pwa/types": "workspace:*",
    "@types/find-in-files": "0.5.3",
    "@types/fs-extra": "11.0.4",
    "@types/node": "20.11.28",
    "eslint-config-shopware": "workspace:*",
    "tsconfig": "workspace:*",
    "typescript": "5.4.3",
    "unbuild": "2.0.0",
    "vitest": "1.4.0"
  }
}
