{
  "name": "@shopware-pwa/cms-base",
  "version": "1.0.1",
  "description": "Vue CMS support for Shopware",
  "author": "Shopware",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shopware/frontends.git"
  },
  "homepage": "https://frontends.shopware.com/getting-started/content-pages.html",
  "keywords": [
    "Shopware",
    "Vue",
    "CMS"
  ],
  "bugs": {
    "url": "https://github.com/shopware/frontends/issues"
  },
  "license": "MIT",
  "type": "module",
  "main": "./nuxt.config.ts",
  "files": [
    "dist",
    "components",
    "helpers",
    "index.cjs",
    "nuxt.config.ts"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "eslint": "eslint components/**/*.vue* --fix --max-warnings=0",
    "lint": "pnpm run eslint && pnpm run typecheck",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@nuxt/kit": "3.12.3",
    "@shopware-pwa/composables-next": "workspace:*",
    "@shopware-pwa/helpers-next": "workspace:*",
    "@shopware/api-client": "workspace:*",
    "@tresjs/cientos": "3.9.0",
    "@tresjs/core": "4.0.2",
    "@vuelidate/core": "2.0.3",
    "@vuelidate/validators": "2.0.4",
    "@vueuse/core": "10.11.0",
    "entities": "5.0.0",
    "html-to-ast": "0.0.6",
    "three": "^0.166.1",
    "vue": "3.4.31"
  },
  "devDependencies": {
    "@nuxt/schema": "3.12.3",
    "@types/three": "^0.166.0",
    "@vitest/coverage-v8": "1.6.0",
    "@vue/eslint-config-typescript": "13.0.0",
    "eslint-config-shopware": "workspace:*",
    "eslint-plugin-vue": "9.27.0",
    "nuxt": "3.12.3",
    "tsconfig": "workspace:*",
    "typescript": "5.5.3",
    "unbuild": "2.0.0",
    "vitest": "1.6.0",
    "vue-eslint-parser": "9.4.3",
    "vue-router": "4.4.0",
    "vue-tsc": "2.0.26"
  }
}
