{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "dist/**",
        ".nuxt/**",
        ".output/**",
        ".vercel/**",
        ".vitepress/dist/**"
      ]
    },
    "generate": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".nuxt/**", ".output/**", ".vercel/**"]
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "test:e2e": {
      "outputs": []
    },
    "test:e2e:chrome": {
      "outputs": []
    } 
  }
}
