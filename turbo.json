{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "prepare": {
      "dependsOn": ["^prepare"],
      "outputs": [".nuxt/**"],
      "cache": false
    },
    "build": {
      "dependsOn": ["^build", "prepare"],
      "outputs": ["dist/**", ".output/**", ".vercel/**", ".vitepress/dist/**"]
    },
    "generate": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".output/**", ".vercel/**"]
    },
    "lint": {
      "cache": false,
      "outputs": [],
      "passThroughEnv": ["TURBO_TELEMETRY_DISABLED", "DO_NOT_TRACK"]
    },
    "lint:fix": {
      "cache": false,
      "outputs": [],
      "passThroughEnv": ["TURBO_TELEMETRY_DISABLED", "DO_NOT_TRACK"]
    },
    "typecheck": {
      "dependsOn": ["^build"],
      "outputs": [],
      "inputs": [
        "src/**/*.ts",
        "src/**/*.vue",
        "*.ts",
        "tsconfig.json",
        "package.json"
      ]
    },
    "generate-types": {
      "cache": false,
      "outputs": []
    },
    "generate-admin-types": {
      "cache": false,
      "outputs": []
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "test:watch": {
      "dependsOn": ["^build"],
      "persistent": true
    },
    "test:bench": {
      "cache": false,
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "test:e2e": {
      "cache": false,
      "outputs": []
    }
  }
}
