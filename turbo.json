{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "dist/**",
        ".nuxt/**",
        ".output/**",
        ".vercel/**",
        ".vitepress/dist/**"
      ]
    },
    "generate": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".nuxt/**", ".output/**", ".vercel/**"]
    },
    "lint": {
      "cache": false,
      "outputs": []
    },
    "generate-types": {
      "cache": false,
      "outputs": []
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "test:watch": {
      "dependsOn": ["^build"],
      "persistent": true
    },
    "test:bench": {
      "cache": false,
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "test:e2e": {
      "outputs": []
    }
  }
}
