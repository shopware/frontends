{
  "name": "shopware-frontends",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "^2.6.0",
    "turbo": "^1.1.9"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "packageManager": "pnpm@6.32.2"
}
